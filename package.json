{
  "name": "WCIFAH-Agent-Payment",
  "version": "1.0.0",
  "description": "Single-Page Stripe Payment App for WhereCanIFindAHome",
  "main": "dist/index.js",
  "scripts": {
    "start": "concurrently \"npm run start:server\" \"npm run start:frontend\"",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:frontend\"",
    "build": "npm run build:server && npm run build:frontend",
    "start:server": "cd server && npm start",
    "start:frontend": "cd frontend && npm run preview",
    "dev:server": "cd server && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build:server": "cd server && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm install && cd server && npm install && cd ../frontend && npm install"
  },
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "^5.5.3"
  },
  "private": true
}
